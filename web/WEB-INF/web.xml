<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>welcome.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>exam.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>exam.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>starttest</servlet-name>
        <servlet-class>exam.starttest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QuestionOperations</servlet-name>
        <servlet-class>exam.QuestionOperations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>timeset</servlet-name>
        <servlet-class>exam.timeset</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveans</servlet-name>
        <servlet-class>exam.saveans</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>exam.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>starttest</servlet-name>
        <url-pattern>/starttest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QuestionOperations</servlet-name>
        <url-pattern>/QuestionOperations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>timeset</servlet-name>
        <url-pattern>/timeset</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveans</servlet-name>
        <url-pattern>/saveans</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-page/>
</web-app>
